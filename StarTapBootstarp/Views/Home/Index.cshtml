@model List<StartImage>
@{
    ViewData["Title"] = "Index";
}

<main>
    <section id="start">
        <div class="container">
            <div class="row text-center">
                <div class="img mt-5">
                    <img src="assets/img/@Model" alt="">
                </div>
                <div class="title mt-3">
                    <h1>@layoutservis.GetDatas().Result.FirstOrDefault(s=>s.Key=="StartName").Value</h1>
                </div>

                <div class="icon mt-0">
                    <i class="fa-solid fa-star"></i>
                </div>

                <div class="info">
                    <span>@layoutservis.GetDatas().Result.FirstOrDefault(s => s.Key == "StartInfo").Value</span>
                </div>
            </div>
        </div>
    </section>
    <div id="portfolio">
        <div class="container">
            <div class="row text-center">
                <div class="title">
                    <h2>PORTFOLIO</h2>
                </div>
                <div class="icon mt-0">
                    <i class="fa-solid fa-star"></i>
                </div>
                <div class="card   col-lg-4">
                    <div class="icon">
                        <i class="fa-solid fa-plus"></i>
                    </div>
                    <img class="rounded" src="assets/img/cabin.png" alt="">
                </div>
                <div class="card  col-lg-4">
                    <img class="rounded" src="assets/img/cake.png" alt="">
                </div>
                <div class="card col-lg-4">
                    <img class="rounded" src="assets/img/circus.png" alt="">
                </div>
                <div class="card col-lg-4">
                    <img class="rounded" src="assets/img/game.png" alt="">
                </div>
                <div class="card col-lg-4">
                    <img class="rounded" src="assets/img/safe.png" alt="">
                </div>
                <div class="card col-lg-4">
                    <img class="rounded" src="assets/img/submarine.png" alt="">
                </div>
            </div>
        </div>
    </div>
    <section id="about">
        <div class="container">
            <div class="row text-center">
                <div class="title mt-5">
                    <h2>ABOUT</h2>
                </div>
                <div class="icon mt-0">
                    <i class="fa-solid fa-star"></i>
                </div>
                <div class="info mt-4 d-flex ">
                    <div class="col-lg-6 text">
                        Freelancer is a free bootstrap theme created by Start Bootstrap. The
                        download includes the
                        complete source files including HTML, CSS, and JavaScript as well as optional SASS
                        stylesheets for easy customization.
                    </div>
                    <div class="col-lg-6 text">
                        You can create your own custom avatar for the masthead, change the
                        icon in the dividers, and
                        add your email address to the contact form to make it fully functional!
                    </div>
                </div>
                <div class="mt-4 ">

                    <button class=" rounded freebtn">
                        <i class="fa-solid fa-download mx-2"></i>Free
                        Download!
                    </button>

                </div>
            </div>
        </div>
    </section>
    <section id="contact">
        <div class="container">
            <div class="row ">
                <div class="titleC text-center">
                    <div class="title col-sm-12">
                        <h2>CONTACT ME</h2>
                    </div>
                    <div class="icon mt-0">
                        <i class="fa-solid fa-star"></i>
                    </div>
                </div>


                <div class="form">
                    <form action="">
                        <div>

                            <input class="form-control w-50 border-end-0 border-top-0 border-start-0 mt-3 "
                                   type="text" id="name" placeholder="Full Name">
                        </div>
                        <div>

                            <input class="form-control w-50 border-end-0 border-top-0 border-start-0 mt-3"
                                   type="text" id="Email" placeholder="Email Address">
                        </div>
                        <div>

                            <input class="form-control w-50 border-end-0 border-top-0 border-start-0" type="text"
                                   id="Phone" placeholder="Phone Number">
                        </div>
                        <div>

                            <input class="form-control w-50 border-end-0 border-top-0 border-start-0" type="text"
                                   id="Message" placeholder="Message">
                        </div>
                        <button class="btn btn-success mt-4">Send</button>
                    </form>

                </div>

            </div>
        </div>
    </section>

</main>

